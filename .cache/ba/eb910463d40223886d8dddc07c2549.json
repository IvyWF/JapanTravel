 0;\n    width: 100%;\n    z-index: 1;\n    margin-bottom: 15px;\n\n    .pagesNav {\n\n        ul {\n            display: flex;\n            justify-content: space-evenly;\n            list-style-type: none;\n\n            li {\n                font-family: 'Pacifico', cursive;\n                letter-spacing: -1px;\n                font-size: 36px;\n\n                a {\n                    color: $navColor;\n                    text-decoration: none;\n\n                    &:hover {\n                        cursor: pointer;\n                    }\n\n                    .letter {\n                        text-shadow: 0.03em 0.03em 0.02em #777;\n                        will-change: transform;\n                        display: inline-block;\n                        transition: transform 0.2s ease;\n\n                        &:hover {\n                            transform: scaleX(0.9) scaleY(1.3);\n                        }\n                    }\n                }\n            }\n        }\n\n    }\n}\n\n.cursor {\n    pointer-events: none;\n\n    &__ball {\n        position: fixed;\n        top: 0;\n        left: 0;\n        mix-blend-mode: difference;\n        z-index: 10000;\n\n        & circle {\n            fill: #ad5389;\n        }\n    }\n}","sass/main.scss":"@import \"./pages/osaka\";\n\n@import url('https://fonts.googleapis.com/css2?family=Chokokutai&display=swap');\n/* font-family: 'Chokokutai', cursive; */\n@import url('https://fonts.googleapis.com/css2?family=Pacifico&display=swap');\n/* font-family: 'Pacifico', cursive; */\n\n@font-face {\n    font-family: 'mijuregular';\n    src: url('../fonts/miju-webfont.woff2') format('woff2'),\n        url('../fonts/miju-webfont.woff') format('woff');\n    font-weight: normal;\n    font-style: normal;\n}\n\n\n\n//Responsive\n$breakpoints: (\n    mobile: 767px,\n    tablet: 992px\n);\n\n@mixin mobile-only {\n    @media screen and (max-width: map-get($breakpoints, mobile)) {\n        @content;\n    }\n}\n\n@mixin tablet-only {\n    @media screen and (min-width: 768px) and (max-width: map-get($breakpoints, tablet)) {\n        @content;\n    }\n}\n\n$redColor: rgba(244, 67, 54, 1);\n$blueColor: rgba(33, 150, 243, 1);\n$navColor: #212E53;\n\n* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n}\n\n.fullscreen {\n    width: 100vw;\n    height: 100vh;\n    overflow: hidden;\n    position: relative;\n\n    .homeTitle {\n        text-align: center;\n        margin-top: 25px;\n        font-family: 'mijuregular', serif, Arial, Helvetica, sans-serif;\n        font-size: 56px;\n        color: #212E53; // #F9968B\n    }\n\n    .homeNav {\n        //menu on the left and in the middleof the page\n        /*margin-left: 40px;\n        position: absolute;\n        top: 50%;\n        -ms-transform: translateY(-50%);\n        transform: translateY(-50%);*/\n        margin-top: 14%;\n\n        @include tablet-only {\n            margin-top: 46%;\n        }\n\n        @include mobile-only {\n            margin-top: 24%;\n        }\n\n        &__ul {\n            list-style-type: none;\n            display: flex;\n            justify-content: space-evenly;\n            align-items: center;\n\n            @include mobile-only {\n                display: flex;\n                flex-direction: column;\n            }\n\n            li {\n                margin-top: 20px;\n                font-size: 50px;\n                font-family: 'Pacifico', cursive;\n                letter-spacing: 5px;\n\n                @include mobile-only {\n                    font-size: 36px;\n                }\n\n                a {\n                    text-decoration: none;\n                    color: #212E53;\n                    transition: 0.3s;\n\n                    &:hover {\n                        cursor: pointer;\n                        color: #fff;\n                        text-shadow: 0 0 5px #03e9f4,\n                            0 0 25px #03e9f4,\n                            0 0 50px #03e9f4,\n                            0 0 100px #03e9f4;\n                    }\n                }\n            }\n\n\n        }\n\n    }\n}\n\n//Social media icons\n//     .social-media {\n//         //icons on the bottom and on the left of the page\n//         /*margin-left: 31px;\n//         position: absolute;\n//         top: 92%;\n//         -ms-transform: translateY(-50%);\n//         transform: translateY(-92%);\n//         display: flex;*/\n\n//         display: flex;\n//         justify-content: center;\n//         align-items: center;\n//         margin-top: 14%;\n\n//         @include tablet-only {\n//             margin-top: 45%;\n//         }\n\n//         @include mobile-only {\n//             margin-top: 33%;\n//         }\n\n//         i {\n//             font-size: 42px;\n//             //margin-left: 17px;\n//             cursor: pointer;\n\n//             @include mobile-only {\n//                 font-size: 32px;\n//             }\n\n//             /*&.icon-3d {\n//                 padding: 10px;\n//                 -webkit-animation: icon3d 200ms 10;\n//                 animation: icon3d 200ms 10;\n\n//                 &:hover {\n//                     -webkit-animation: icon3d 200ms infinite;\n//                     animation: icon3d 200ms infinite;\n//                 }\n//             }*/\n\n//             &.fa-facebook-f {\n//                 margin-left: 50px;\n//             }\n\n//             &.fa-pinterest-p {\n//                 margin-left: 50px;\n//             }\n//         }\n\n\n\n//     }\n\n// }\n\n// @keyframes icon3d {\n//     0% {\n//         text-shadow: 5px 4px $redColor, -5px -6px $blueColor;\n//     }\n\n//     25% {\n//         text-shadow: -5px -6px $redColor, 5px 4px $blueColor;\n//     }\n\n//     50% {\n//         text-shadow: 5px -4px $redColor, -8px 4px $blueColor;\n//     }\n\n//     75% {\n//         text-shadow: -8px -4px $redColor, -5px -4px $blueColor;\n//     }\n\n//     100% {\n//         text-shadow: -5px 0 $redColor, 5px -4px $blueColor;\n//     }\n// }\n\n.fullscreen .video-background {\n    position: absolute;\n    width: auto;\n    height: auto;\n    min-width: 100%;\n    min-height: 100%;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    z-index: -1;\n    filter: blur(7px);\n\n\n    @media (min-aspect-ratio: 16/9) {\n        .video-background {\n            width: 100%;\n            height: auto;\n        }\n    }\n\n    @media (max-aspect-ratio: 16/9) {\n        .video-background {\n            width: auto;\n            height: 100%;\n        }\n    }\n}\n\n\n//Osaka Page\n\n\n\n// .bd-image {\n//     position: fixed;\n//     background-image: url('/images/sunrise.jpg');\n//     //filter: blur(15px);\n//     opacity: 65%;\n//     -webkit-filter: blur(8px);\n//     height: 100%;\n//     background-position: center;\n//     background-repeat: no-repeat;\n//     background-size: cover;\n// }"},"lineCount":null}},"error":null,"hash":"fd132d7e2f4ee390f6ebac0c52ffe792","cacheData":{"env":{}}}